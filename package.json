    "gh-pages": "^6.0.0",
    "jest-mock": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jitsi": "^1.0.4",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "redux-mock-store": "^1.5.4",
    "uuid": "^9.0.1"
